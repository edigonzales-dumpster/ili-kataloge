    CLASS Biotopbaum =
      Geometrie : GeometryCHLV95_V1.Coord2;
      Baum_ID : TEXT*50;
      Baumart : TEXT*255;
      Merkmal : SO_AWJF_Biotopbaeume_20991231.Kataloge.Kat_Hauptmerkmal_Ref;
    END Biotopbaum;

  END Biotopbaeume;

